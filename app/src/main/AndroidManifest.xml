<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.housekeeperapplication">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/my_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.WelcomeActivity">

            <!--
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            -->
        </activity>
        <activity android:name=".ui.MainActivity" />
        <activity
            android:name=".ui.ShowTelTypeActivity"
            android:label="电话通讯录" />
        <activity android:name=".ui.ShowTelNumActivity" />
        <activity android:name=".ui.IntroduceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".ui.MusicService" />

        <activity
            android:name=".ui.SettingActivity"
            android:label="设置" />
        <activity
            android:name=".ui.SoftMgrActivity"
            android:label="手机管理" />
        <activity
            android:name=".ui.ShowSoftActivity"
            android:label="软件管理" />
        <activity
            android:name=".ui.CleanProcessActicity"
            android:label="手机加速" />
        <activity
            android:name=".ui.PhonemgrActivity"
            android:label="硬件配置" />
        <activity android:name=".ui.FileMgrActivity"></activity>
    </application>

</manifest>